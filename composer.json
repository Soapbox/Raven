{
    "name": "soapbox/soapbox-vagrant",
    "description": "A virtual machine for SoapBoxers.",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:jshayes/raven-api.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.0",
        "symfony/process": "~2.0",
        "herrera-io/phar-update": "1.*",
        "jshayes/raven-api": "dev-master"
    },
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul@soapboxhq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SoapBox\\Raven\\": "src/"
        }
    },
    "bin": [
        "homestead"
    ]
}
